@model List<Departamento>


<ul class="navbar-nav">
    <li class="nav-itemdropdown">
        <button class="btn btn-dark dropdown-toggle" data-bs-toggle="dropdown" aria-expanded="false">Departamentos</button>
        <ul class="dropdown-menu dropdown-menu-dark">
            @foreach(Departamento dept in Model){
                <li>
                    <a class="dropdown-item" asp-controller="Paginacion" asp-action="EmpleadosDepartamento" asp-route-iddepartamento="@dept.IdDepartemento">@dept.Nombre</a>
                </li>
            }
        </ul>
    </li>
</ul>
